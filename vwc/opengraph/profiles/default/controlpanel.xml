<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="vwc.opengraph"
    purge="False">

    <configlet
        title="OpenGraph"
        action_id="opengraph"
        appId="vwc.opengraph"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@opengraph-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>